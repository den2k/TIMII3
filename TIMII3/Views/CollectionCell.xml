<UICollectionViewCell clipsToBounds="true">
    
    <UIImageView
    contentMode="scaleAspectFit"
    top=""
    left="50% - width/2"
    height="65%"
    width="65%"
    image="{image}"
    tintColor=""
    />
    
    <UILabel
    text="Timer {row+1}"
    top="previous.bottom + 5"
    left="50% - width/2"
    height="auto"
    width="auto"
    backgroundColor="// text label background color"
    font="Avenir Next 16"
    textColor="white"
    />
    
</UICollectionViewCell>
