<UITabBarController>
    <UITabBar
        alpha="0.95"
        isTranslucent="false"
        barTintColor="softWhite"
        tintColor="eucalyptus"
        barStyle="default"
    />
    <UIViewController
        tabBarItem.image="timii"
        tabBarItem.title="Dashboard"
        tabBarItem.imageInsets.top="0"
        tabBarItem.imageInsets.left="0"
        tabBarItem.imageInsets.bottom="0"
        tabBarItem.imageInsets.right="0"
        xml="DashboardScreenViewController.xml"
    />
    <UIViewController
        tabBarItem.image="setting"
        tabBarItem.title="Setting"
        tabBarItem.imageInsets.top="0"
        tabBarItem.imageInsets.left="0"
        tabBarItem.imageInsets.bottom="0"
        tabBarItem.imageInsets.right="0"
        xml="SettingScreenViewController.xml"
    />
    
</UITabBarController>
