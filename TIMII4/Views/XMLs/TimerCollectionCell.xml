        image="{image}"
        contentMode="scaleAspectFit"
        top="10"
        left="50% - width/2"
        height="65%"
        width="65%"
        />
        
    <UILabel
        text="{title}"
        top="previous.bottom + 5"
        left="50% - width/2"
        height="auto"
        width="auto"
        backgroundColor="// text label background color"
        font="Avenir Next 16"
        textColor="gray"
        />
 
